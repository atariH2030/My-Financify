{
  "name": "financy-life",
  "version": "3.12.0",
  "description": "Sistema de gest√£o financeira pessoal com arquitetura moderna, robusta e otimizada",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:health": "playwright test health-check.spec.ts",
    "test:health:headed": "playwright test health-check.spec.ts --headed",
    "test:mock": "playwright test auth-mock.spec.ts",
    "test:mock:headed": "playwright test auth-mock.spec.ts --headed",
    "test:report": "playwright show-report test-results/playwright-report",
    "check:i18n": "tsx scripts/detect-untranslated.ts",
    "check:i18n:report": "tsx scripts/detect-untranslated.ts && cat docs/UNTRANSLATED_REPORT_*.md"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "@types/chart.js": "^2.9.41",
    "@types/lodash": "^4.14.202",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^1.8.29",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "framer-motion": "^12.23.24",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "otpauth": "^9.4.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "recharts": "^3.5.1",
    "uuid": "^13.0.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.25",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.10",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.10",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "keywords": [
    "financeiro",
    "gestao",
    "typescript",
    "vite",
    "performance"
  ],
  "author": "Rickson",
  "license": "MIT"
}
